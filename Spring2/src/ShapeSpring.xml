<?xml version="1.0" encoding="UTF-8"?>


<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="shape" class="com.beans.Shape">
    <property name="triangle" ref="tr" />
    <property name="rectangle" ref="rect" />
</bean>
<bean id="tr" class="com.beans.Triangle" >
	<property name="pointA" value="10"></property>
	<property name="pointB" value="20"></property>
	<property name="pointC" value="30"></property>
	
</bean>
<bean id="rect" class="com.beans.Rectangle" >
	<property name="len" value="100"></property>
	<property name="bre" value="200"></property>
	
	
</bean>

</beans>


